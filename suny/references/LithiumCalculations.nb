      RowBox[{
       RowBox[{"-", "2"}], 
       SqrtBox[
        RowBox[{
         SubscriptBox["V", "0"], "/", 
         SubscriptBox["E", "r"]}]]}], ")"}]}]}], TraditionalForm]],
