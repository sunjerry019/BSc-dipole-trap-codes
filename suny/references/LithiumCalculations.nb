  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"t", " "}], 
     SubscriptBox["E", "r"]], "=", 
    RowBox[{
     FractionBox["4", 
      SqrtBox["\[Pi]"]], 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        SubscriptBox["V", "1"], 
        SubscriptBox["E", "r"]], ")"}], 
      RowBox[{"3", "/", "4"}]], 
